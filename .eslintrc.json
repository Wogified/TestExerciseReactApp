{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "linebreak-style": 0,
    // "indent": ["error", 4],
    // "arrow-parens": ["error", "always"],
    "prettier/prettier": "error"
  }
}
